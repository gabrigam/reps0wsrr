<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>
	TAITest</display-name>
	<servlet>
		<description>
		</description>
		<display-name>
		PrintUserInfo</display-name>
		<servlet-name>PrintUserInfo</servlet-name>
		<servlet-class>
		examples.was.taitest.PrintUserInfo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PrintUserInfo</servlet-name>
		<url-pattern>/PrintUserInfo</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<security-constraint>
		<display-name>
		PrintUserInfoConstraint</display-name>
		<web-resource-collection>
			<web-resource-name>GetUserInfoWebRsrc</web-resource-name>
			<url-pattern>/PrintUserInfo</url-pattern>
			<http-method>GET</http-method>
			<http-method>POST</http-method>
		</web-resource-collection>
		<auth-constraint>
			<description>
			PrintUserInfoAuthRole</description>
			<role-name>PrintUserInfoRole</role-name>
		</auth-constraint>
	</security-constraint>
	<login-config>
		<auth-method>FORM</auth-method>
		<form-login-config>
			<form-login-page>/login.jsp</form-login-page>
			<form-error-page>/login.jsp</form-error-page>
		</form-login-config>
	</login-config>
	<security-role>
		<description>
		Security Role for PrintUserInfo</description>
		<role-name>PrintUserInfoRole</role-name>
	</security-role>
</web-app>